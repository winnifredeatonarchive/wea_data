<?xml version="1.0" encoding="UTF-8"?>
<project name="buildDocSite" default="all">
    
    <include file="../buildGlobals.xml"/>
  
  <target name="init">
      <delete dir="${doc.site.dir}"/>
      <mkdir dir="${doc.site.dir}"/>
  </target>
    
    <target name="createSite">
        <copy file="${project.dir}/documentation/documentation.html" todir="${doc.site.dir}"/>
    </target>
  
  <target name="all" depends="init, createSite"/>
  
</project>