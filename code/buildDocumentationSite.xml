<?xml version="1.0" encoding="UTF-8"?>
<project name="buildDocSite" default="all">
    
    <include file="../buildGlobals.xml"/>
  
  <target name="init">
      <delete dir="${docSiteTemp}"/>
      <mkdir dir="${docSiteTemp}"/>
  </target>
    
    <target name="createSite">
        <copy file="${project.dir}/documentation/documentation.html" todir="${docSiteTemp}"/>
    </target>
  
  <target name="all" depends="init, createSite"/>
  
</project>