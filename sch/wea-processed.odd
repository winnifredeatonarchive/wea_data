          <catDesc>Géneral</catDesc>
        </category>
        <category>
          <catDesc>Journal</catDesc>
        </category>
        <category>
          <catDesc>Manuel technique</catDesc>
        </category>
      </category>
    </egXML>
  </exemplum>
  <exemplum xml:lang="zh-TW">
    <egXML xmlns="http://www.tei-c.org/ns/Examples">
