        <elementRef key="sourceDesc" minOccurs="0" maxOccurs="unbounded"/>
      </sequence>
      <sequence>
        <elementRef key="fileDesc"/>
        <elementRef key="profileDesc"/>
      </sequence>
    </alternate>
  </content>
