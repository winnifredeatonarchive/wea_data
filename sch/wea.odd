                        </sch:assert>
                           </sch:rule>
                        </sch:pattern>
                     </constraint>
                  </constraintSpec>
    
               </elementSpec>
               
               <elementSpec ident="biblScope" module="core" mode="change">
                  <attList>
                     <attDef ident="unit" mode="replace" usage="req">
                        <valList type="closed">
                           <valItem ident="volume" mode="add">
                              <desc>volume</desc>
                              <gloss>the element contains a volume number</gloss>
                           </valItem>
                           <valItem ident="issue" mode="add">
