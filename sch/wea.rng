                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Hilary Ball) HB1</a:documentation>
                  <value>pers:XY1</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Xine Yao) XY1</a:documentation>
                  <value>pers:PP1</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Pavlina Pajot) PP1</a:documentation>
