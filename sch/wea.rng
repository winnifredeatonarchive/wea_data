                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Jean Lee Cole) JLC1</a:documentation>
                  <value>pers:SL1</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Sydney Lines) SL1</a:documentation>
                  <value>pers:KI1</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Ken Ip) KI1</a:documentation>
                  <value>pers:JT1</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Joey Takeda) JT1</a:documentation>
                  <value>pers:SP1</value>
                  <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(Serina Patterson) SP1</a:documentation>
